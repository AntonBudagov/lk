mixin fourPlus
    a.fourPlus-item.sectionContent-item.active(href="#")
        i.icon.icon-le-smart-price
        h4 РАЗУМНЫЕ ЦЕНЫ
    a.fourPlus-item.sectionContent-item(href="#")
        i.icon.icon-le-garant
        h4 Гарантии качества
    a.fourPlus-item.sectionContent-item(href="#")
        i.icon.icon-le-human
        h4 Квалифицированные авторы
    a.fourPlus-item.sectionContent-item(href="#")
        i.icon.icon-le-short-time
        h4 Короткие сроки

mixin fourCircle
    .fourCircle
        .fourCircleContent
            .fourCircleContent-item
                .step
                    span 1
                h4 ОСТАВЬТЕ ЗАЯВКУ
            .fourCircleContent-item
                .step
                    span 2
                h4 ОЦЕНКА СТОИМОСТИ И СРОКОВ
            .fourCircleContent-item
                .step
                    span 3
                h4 ВЫПОЛНЕНИЕ РАБОТЫ
            .fourCircleContent-item
                .step
                    span 4
                h4 ПОЛУЧЕНИЕ ЗАКАЗА

